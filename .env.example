# ===== Core OpenAI-Compatible LLM Settings =====
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1

# Preferred primary chat/completions model (will be validated)
OPENAI_MODEL=gpt-4o-mini

# Optional: comma-separated fallback models. First that probes successfully is used.
FALLBACK_MODELS=gpt-4o-mini,gpt-4o,o4-mini,o3-mini

# ===== Embedding Settings =====
EMBEDDING_MODEL=text-embedding-3-small
# EMBEDDING_API_BASE=https://api.openai.com/v1   # Uncomment to override base for embeddings

# ===== Qdrant Vector DB =====
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=http://localhost:6333

# ===== Production API Keys for Enhanced Data Sources =====
# Twitter API (for enhanced sentiment analysis) - Bearer token from Twitter Developer Portal
# NOTE: Twitter scraping uses twscrape which doesn't require API keys, so this is optional
# TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Financial Modeling Prep (FMP) - Comprehensive financial data
FINANCIALMODELINGPREP_API_KEY=your_fmp_api_key_here

# Finnhub - Real-time stock quotes and news
FINNHUB_API_KEY=your_finnhub_api_key_here

# TwelveData - High-quality market data and technical indicators
TWELVEDATA_API_KEY=your_twelvedata_api_key_here

# Alpha Vantage - Stock market data and fundamental analysis
ALPHAVANTAGE_API_KEY=your_alphavantage_api_key_here

# Polygon.io - Advanced market data, options, and analytics
POLYGON_API_KEY=your_polygon_api_key_here

# ===== Social Media and Alternative Data =====
# Reddit API (for additional sentiment analysis)
REDDIT_USER_AGENT=your_reddit_app_name/1.0
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# ===== API Configuration =====
# Timeout settings for external API calls (in seconds)
API_REQUEST_TIMEOUT=30
API_MAX_RETRIES=3

# Rate limiting settings
API_RATE_LIMIT_DELAY=1.0

# ===== Fallback Configuration =====
# Enable fallback to free-tier APIs when premium keys are unavailable
ENABLE_API_FALLBACK=true

# ===== Optional API Keys (for extended functionality) =====
# Additional market data providers
IEX_API_KEY=your_iex_api_key_here
TRADINGECONOMICS_API_KEY=your_tradingeconomics_api_key_here
FRED_API_KEY=your_fred_api_key_here

# News and sentiment data providers
NEWSAPI_API_KEY=your_newsapi_api_key_here
GOOGLE_NEWS_API_KEY=your_google_news_api_key_here

# ===== Environment Configuration =====
# Set to 'production' for production deployments
ORACLE_ENV=development

# Debug mode (set to 'true' for development)
DEBUG=false

# Logging level
LOG_LEVEL=INFO

# ===== Database Configuration =====
ACCOUNTS_DB_PATH=data/databases/accounts.db
MODEL_MONITORING_DB_PATH=data/databases/model_monitoring.db
PROMPT_OPTIMIZATION_DB_PATH=data/databases/prompt_optimization.db
CACHE_DB_PATH=data/databases/cache.db

# ===== Instructions =====
# 1. Copy this file to .env
# 2. Replace all placeholder values with your actual API keys
# 3. Remove any lines for services you don't plan to use
# 4. Never commit .env files to version control
# 5. For production deployments, ensure all required API keys are set
# Cache Warming Configuration
CACHE_WARMING_ENABLED=true
WARM_UP_AT_MARKET_OPEN=true
WARM_UP_HOURLY=true
WARM_UP_INTERVAL_MINUTES=60
CACHE_WARMING_VOLUME_THRESHOLD=1000000
# 6. Some APIs offer free tiers with limitations - upgrade to paid plans for full functionality
# ===== Cache Configuration =====
# Redis settings for enhanced caching performance
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_SSL=false
REDIS_TIMEOUT=5

# Cache TTL settings (in seconds)
REDIS_TICKER_DATA_TTL=86400  # 24 hours for popular ticker data
REDIS_MARKET_DATA_TTL=3600   # 1 hour for market data
REDIS_SENTIMENT_TTL=1800     # 30 minutes for sentiment data
REDIS_NEWS_TTL=900          # 15 minutes for news data

# Cache size limits
REDIS_MAX_MEMORY=256mb
REDIS_EVICTION_POLICY=allkeys-lru

# Enable Redis caching (set to false to disable Redis and use only memory cache)
ENABLE_REDIS_CACHE=true